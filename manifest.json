{
    "manifest_version": 2,
    "background": {
        "page": "background.html"
    },
    "name": "Bug Shooter",
    "description": "Shoot your bug and send photo.",
    "version": "0.0.1",
    "icons": {
        "16": "img/16.png",
        "48": "img/48.png",
        "128": "img/128.png"
    },
    "browser_action": {
        "default_icon": "img/16.png"
    },
    "optional_permissions": [ "clipboardRead" ],
    "permissions": ["tabs", "file://*/*", "http://*/*", "https://*/*", "notifications"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Shift+Alt+0"

      }
    },
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+1"
       
      }
    }
  }
	
}